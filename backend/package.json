{
  "name": "swami-rupeshwaranand-backend",
  "version": "1.0.0",
  "description": "AWS Lambda-based NestJS backend with DynamoDB and Cognito",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:offline": "sls offline start --stage local",
    "deploy": "sls deploy --stage prod",
    "deploy:dev": "sls deploy --stage dev",
    "remove": "sls remove --stage prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:local": "bash scripts/seed-local-data.sh",
    "seed:subscription-plans": "IS_LOCAL=true npx ts-node scripts/seed-subscription-plans.ts",
    "seed:subscription-plans:prod": "npx ts-node scripts/seed-subscription-plans.ts",
    "seed:pages": "IS_LOCAL=true npx ts-node scripts/seed-pages.ts",
    "seed:pages:prod": "npx ts-node scripts/seed-pages.ts",
    "create:super-admin": "IS_LOCAL=true npx ts-node scripts/create-super-admin.ts",
    "create:super-admin:prod": "npx ts-node scripts/create-super-admin.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.500.0",
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/client-ses": "^3.984.0",
    "@aws-sdk/lib-dynamodb": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@aws-sdk/util-dynamodb": "^3.500.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-jwt-verify": "^5.1.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mongodb": "^6.3.0",
    "nodemailer": "^6.9.8",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.8",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.2",
    "serverless-plugin-warmup": "^8.3.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
